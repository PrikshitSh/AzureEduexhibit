<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>connection.SignUp</servlet-class>
    </servlet>
   
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SignUpTeacher</servlet-name>
        <servlet-class>connection.SignUpTeacher</servlet-class>
    </servlet>
   
    <servlet-mapping>
        <servlet-name>SignUpTeacher</servlet-name>
        <url-pattern>/SignUpTeacher</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>connection.Login</servlet-class>
    </servlet>
   
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginT</servlet-name>
        <servlet-class>connection.LoginT</servlet-class>
    </servlet>
   
    <servlet-mapping>
        <servlet-name>LoginT</servlet-name>
        <url-pattern>/LoginT</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginAdmin</servlet-name>
        <servlet-class>connection.LoginAdmin</servlet-class>
    </servlet>
   
    <servlet-mapping>
        <servlet-name>LoginAdmin</servlet-name>
        <url-pattern>/LoginAdmin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>connection.Logout</servlet-class>
    </servlet>
   
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UploadProject</servlet-name>
        <servlet-class>connection.UploadProject</servlet-class>
    </servlet>
   
    <servlet-mapping>
        <servlet-name>UploadProject</servlet-name>
        <url-pattern>/UploadProject</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
